" 关闭自带的智能提示，使用deoplete的提示功能
let g:jedi#completions_enabled = 0

let g:jedi#goto_command = "<silent>gi"
let g:jedi#goto_assignments_command = "<silent>g"
let g:jedi#goto_definitions_command = "<silent>gd"
let g:jedi#documentation_command = "K"
let g:jedi#usages_command = "<silent>fs"
let g:jedi#completions_command = "<C-Space>"
let g:jedi#rename_command = "<silent>F2"
